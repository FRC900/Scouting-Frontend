<h1>Listing match_categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Match form</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @match_categories.each do |match_category| %>
      <tr>
        <td><%= match_category.name %></td>
        <td><%= match_category.match_form %></td>
        <td><%= link_to 'Show', match_category %></td>
        <td><%= link_to 'Edit', edit_match_category_path(match_category) %></td>
        <td><%= link_to 'Destroy', match_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Match category', new_match_category_path %>
