<div>
	<p>
	  <%= f.label :name, "Category Name" %><br />
	  <%= f.text_area :name %><br />
	  <%= f.hidden_field :_destroy %>
	  <%= link_to "remove", '#', class: "remove_fields" %>
	</p>
	<%= f.fields_for :match_form_lineitems do |builder| %>
	  <%= render 'match_form_lineitem_fields', :f => builder %>
	<% end %>
	<p><%= link_to_add_fields "Add Lineitem", f, :match_form_lineitems %></p>
</div>